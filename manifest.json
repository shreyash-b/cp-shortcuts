{
    "manifest_version": 3,
    "name": "CP Shortcut Keys",
    "version": "1.0",
    "description": "Easily Run and Submit Code on Competitive Programming Websites without leaving keyboard. Use ctrl+alt+r to Run and ctrl+alt+s to Submit Code.",


    "content_scripts": [{
        "matches": ["https://*.hackerrank.com/*"],
        "js": ["js/sites/hackerrank.js", "js/main.js"]
    }, {
        "matches": ["https://*.codechef.com/*"],
        "js": ["js/sites/codechef.js", "js/main.js"]
    }],

    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}